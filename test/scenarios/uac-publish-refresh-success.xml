<scenario name="Basic Sipstone UAC">
  <Global variables="etag" />
  <!-- In client mode (sipp placing calls), the Call-ID MUST be         -->
  <!-- generated by sipp. To do so, use [call_id] keyword.                -->
  <send retrans="500">
    <![CDATA[

      PUBLISH sip:resource@drachtio.org SIP/2.0
      Via: SIP/2.0/UDP 10.0.1.7:53980;branch=z9hG4bK-524287-1---f3dac96e977a105b;rport
      Max-Forwards: 70
      To: <sip:resource@drachtio.org>
      From: <sip:resource@drachtio.org>;tag=cc983470
      Call-ID: [call_id]
      CSeq: 1 PUBLISH
      SIP-If-Match: [$etag]
      Expires: 30
      Allow: SUBSCRIBE, NOTIFY, INVITE, ACK, CANCEL, BYE, REFER, INFO, OPTIONS, MESSAGE
      User-Agent: Bria 5 release 5.1.0 stamp 89373
      Event: presence
      Content-Length: 0

    ]]>
  </send>

  <recv response="200">
    <action>
      <log message="IGNORE: [$etag]"/>
    </action>
  </recv>

</scenario>

